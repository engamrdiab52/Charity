<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amrabdelhamiddiab.charity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_charity"
        android:name=".CharityApp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_charity_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Charity">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Charity.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>